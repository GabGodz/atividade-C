#include <stdio.h>

int main() {
    // Variáveis
    int numero;
    int soma = 0;

    // Laço para solicitar 5 números
    for (int i = 1; i <= 5; i++) {
        printf("Informe o %do número inteiro: ", i);
        scanf("%d", &numero);

        // Adiciona o número à soma
        soma += numero;
    }

    // Exibe a soma
    printf("A soma dos números é: %d\n", soma);

    return 0;
}
